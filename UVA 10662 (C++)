#include <bits/stdc++.h>
#define ll long long
#define forit(it, a) for(__typeof(a.begin()) it = a.begin(); it != a.end(); it++)
#define pb push_back
#define mp make_pair
using namespace std;
#define rep(i, a, n) for (int i = a; i < n; i++)

int main(){

	int n; cin >> n;
	while (true){
		int arr[n];
		for (int i = 0; i < n; i++) cin >> arr[i];

		rep (i, 0, n)
		rep (j, i+1, n)
		rep (k, j+1, n)
		rep (l, k+1, n)
		rep (m, l+1, n)
		rep (na, m+1, n)
		printf("%d %d %d %d %d %d\n", arr[i], arr[j], arr[k], arr[l], arr[m], arr[na]);
		
		cin >> n;
		if (n == 0) break;
		cout << endl;
	}
	
	return 0;
}
